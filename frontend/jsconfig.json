{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*",
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
    },
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
  },
  "exclude": [
    "node_modules"
  ]
}